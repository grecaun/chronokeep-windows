<Window x:Class="Chronokeep.UI.API.APIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chronokeep.UI.API"
        mc:Ignorable="d"
        Title="Link Event"
        d:DataContext="{d:DesignInstance local:APIWindow, IsDesignTimeCreatable=True}"
        WindowStartupLocation="CenterScreen"
        SizeToContent="Height"
        >
    <Grid>
        <Grid Margin="10">
            <Frame x:Name="APIFrame" NavigationUIVisibility="Hidden" IsTabStop="False" />
        </Grid>
    </Grid>
</Window>
